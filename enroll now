  <script>
    document.getElementById("yr").textContent = new Date().getFullYear();

    function toast(msg){
      const t = document.getElementById("toast");
      t.textContent = msg;
      t.style.display = "block";
      clearTimeout(window.__t);
      window.__t = setTimeout(()=>{ t.style.display="none"; }, 2500);
    }

    function copyPromo(){
      const msg = `Hi! If you want the FREE AI starter prompts + the full AI Creator Mastery System link, comment "AI" or DM me "AI".`;
      navigator.clipboard.writeText(msg).then(()=>toast("Copied promo message ‚úÖ"));
    }
  </script>
