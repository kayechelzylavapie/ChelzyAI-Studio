  <div class="toast" id="toast"></div>

  <script>
    document.getElementById("yr").textContent = new Date().getFullYear();

    function toast(msg){
      const t = document.getElementById("toast");
      t.textContent = msg;
      t.style.display = "block";
      clearTimeout(window.__t);
      window.__t = setTimeout(()=>{ t.style.display="none"; }, 2600);
    }

    function copyDM(){
      const msg = `Hey! If you want the AI Creator Mastery System access, pay via GCash/PayPal on my site, then submit proof. Comment "AI" and I‚Äôll send the link.`;
      navigator.clipboard.writeText(msg).then(()=>toast("DM promo copied ‚úÖ"));
    }

    // ‚úÖ Auto-detect QR image files in your repo so you don't need to rename.
    // It tries common filenames + also tries URL-encoded names with spaces.
    async function pickFirstExisting(candidates) {
      for (const c of candidates) {
        try {
          const res = await fetch(c, { method: "GET", cache: "no-store" });
          if (res.ok) return c;
        } catch (e) {}
      }
      return null;
    }

    (async function initQR(){
      const gcashEl = document.getElementById("gcashQR");
      const paypalEl = document.getElementById("paypalQR");
      const gcashHint = document.getElementById("gcashHint");
      const paypalHint = document.getElementById("paypalHint");

      // Add your likely filenames here; the script will pick the first that exists.
      // If your images have spaces in the name, URL-encoding is required: space => %20
      const gcashCandidates = [
        "gcash.jpg","gcash.jpeg","gcash.png",
        "GCASH.jpg","GCASH.png",
        "gcash-qr.jpg","gcash-qr.png",
        "kaye%20chelzy%20opimo.jpg","kaye%20chelzy%20opimo.png",
        "kaye%20chelzy%20opimo%202.jpg","kaye%20chelzy%20opimo%202.png"
      ];

      const paypalCandidates = [
        "paypal.jpg","paypal.jpeg","paypal.png",
        "PAYPAL.jpg","PAYPAL.png",
        "paypal-qr.jpg","paypal-qr.png",
        "pay%20pal.jpg","pay%20pal.png",
        "kaye%20chelzy%20opimo%20paypal.jpg","kaye%20chelzy%20opimo%20paypal.png"
      ];

      const gcashFile = await pickFirstExisting(gcashCandidates);
      const paypalFile = await pickFirstExisting(paypalCandidates);

      if (gcashFile) {
        gcashEl.src = gcashFile;
        gcashHint.textContent = "If the QR looks blurry, open the image in a new tab and zoom in before scanning.";
      } else {
        gcashEl.style.display = "none";
        gcashHint.innerHTML = "GCash QR not found in the repository. Upload your GCash QR image to this repo (same folder as index.html).";
      }

      if (paypalFile) {
        paypalEl.src = paypalFile;
        paypalHint.textContent = "If the QR looks blurry, open the image in a new tab and zoom in before scanning.";
      } else {
        paypalEl.style.display = "none";
        paypalHint.innerHTML = "PayPal QR not found in the repository. Upload your PayPal QR image to this repo (same folder as index.html).";
      }
    })();
  </script>
</body>
